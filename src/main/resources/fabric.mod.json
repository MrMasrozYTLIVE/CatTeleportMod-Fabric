{
  "schemaVersion": 1,
  "id": "ctpmod",
  "version": "${version}",
  "name": "CatTeleport Mod",
  "description": "This mod allows you to rejoin any server in set period of time.",
  "authors": [
    "Just_S",
    "MiTask"
  ],
  "contact": {
    "homepage": "https://github.com/MrMasrozYTLIVE/CatTeleportMod-Fabric",
    "issues": "https://github.com/MrMasrozYTLIVE/CatTeleportMod-Fabric/issues"
  },

  "license": "MIT",
  "icon": "assets/ctpmod/icon.png",

  "environment": "client",
  "entrypoints": {
    "client": [
      "net.just_s.ctpmod.CTPMod"
    ],
    "modmenu": [
      "net.just_s.ctpmod.config.ModMenuIntegration"
    ]
  },

  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": ">=${minecraft_version}",
    "java": ">=21",
    "cloth-config": ">=${clothconfig_version}"
  }
}
